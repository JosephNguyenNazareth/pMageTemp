[
    {
        "app": "github",
        "action": [
            {
                "name": "push",
                "method": "GET",
                "projectLink": "{server}/{userNameApp}/{projectName}",
                "apiInfo": "api.github.com/repos/{userNameApp}/{projectName}",
                "extraInfo": {
                    "id": "sha",
                    "time": "commit|committer|date",
                    "task": "commit|message",
                    "actor": "commit|committer|name"
                }
            }
        ]
    },
    {
        "app": "gitlab",
        "action": [
            {
                "name": "push",
                "method": "GET",
                "projectLink": "{server}/{userNameApp}/{projectName}",
                "apiInfo": "gitlab.com/api/v4/projects/{userNameApp}%2F{projectLink}/repository/commits",
                "extraInfo": {
                    "id": "id",
                    "time": "created_at",
                    "task": "title",
                    "actor": "committer_name"
                }
            }
        ]
    }
]
